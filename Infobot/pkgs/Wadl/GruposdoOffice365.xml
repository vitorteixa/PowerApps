      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="groupId" type="xs:string" required="true">
        <doc title="Escolha um grupo na lista suspensa ou digite a ID do grupo." />
      </param>
      <method siena:requiresAuthentication="true" name="GET" id="ListGroupMembers" actionName="ListGroupMembers" visibility="advanced" siena:isDeprecated="false">
        <doc title="Listar os membros do grupo&#xA;Esta operação retorna uma lista de todos os membros do grupo especificado e seus detalhes, como nome, cargo, email etc. Você pode consultar até 1000 itens usando o parâmetro Principal. Caso você precise recuperar mais de 1000 valores, ative o recurso Configurações -&gt; Paginação e forneça o Limite." />
        <request>
          <param style="query" name="$top" type="xs:int">
            <doc title="Número de membros do grupo a serem recuperados (de 1 a 999, o padrão é 100)." />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:ListGroupMembers_Response" />
        </response>
      </method>
    </resource>
